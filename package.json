{
  "name": "atoms-utils",
  "author": "pyros.sh",
  "version": "0.9.2",
  "description": "A common aws utils library for react-native and nodejs",
  "license": "MIT",
  "typings": "index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "react-native",
    "atoms",
    "atoms-state",
    "atoms-ui",
    "atoms-utils",
    "state",
    "pyros.sh"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && tsc",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "deploy": "yarn build && cp package.json dist && cd dist && yarn publish --non-interactive"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 160,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/yup": "^0.29.11",
    "dynamodb-local": "^0.0.31",
    "husky": "^5.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "yup": "^0.32.9"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.10.0",
    "@aws-sdk/client-lambda": "^3.11.0",
    "@aws-sdk/client-s3": "^3.10.0",
    "@aws-sdk/s3-request-presigner": "^3.10.0",
    "@aws-sdk/util-dynamodb": "^3.10.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/jwk-to-pem": "^2.0.0",
    "axios": "^0.21.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "text-encoding-polyfill": "^0.6.7"
  }
}
